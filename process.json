{
  "apps" : [{
    "exec_interpreter"   : "node",
    "exec_mode"          : "fork_mode",
    "instances"          : "1",
    "log_date_format"    : "YYYY-MM-DD HH:mm Z",
    "max_memory_restart" : "160",
    "merge_logs"         : true,
    "name"               : "app",
    "script"             : "app.js",
    "cwd"                : ".",
    "node_args"          : "",
    "ignoreWatch"        : ["[\\/\\\\]\\./", "log"],
    "watch"              : true,
    "post_update"        : "npm i > npm_logs; echo TRACE>TRACE"
  }]
}
